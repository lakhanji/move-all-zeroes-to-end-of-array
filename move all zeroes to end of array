#include<bits/stdc++.h>
using namespace std;

class Solution{
public:
	void pushZerosToEnd(int arr[], int n) {
	    int j=0;
	    for(int i=0;i<n;i++){
	        if(arr[i]!= 0){
	            swap(arr[i],arr[j]);
	            j++;
	        }
	    }
	}
};

int main(){
int t;
  cin>>t;
  while(t--){
    int n;
    cin>>n;
    int a[N];
    for(int i=0;i<n;i++){
      cin>>a[i];}
  solution ob;
  ob.pushZerosToEnd(a,n);
  for(int i=0;i<n;i++)
    cout<<a[i]<<" ";
cout<<endl;
  }
  return 0;
}


